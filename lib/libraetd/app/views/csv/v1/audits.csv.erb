<%- headers = [ 'Created At',
                'Work Id',
                'User Id',
                'Message'
              ] -%>
<%= CSV.generate_line headers %>
<%- @records.each do |rec| -%>
   <%= raw( CSV.generate_line( [ rec.created_at,
                            rec.work_id,
                            rec.user_id,
                            rec.what
                          ] ) ) -%>
<%- end -%>