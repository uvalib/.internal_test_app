<tr class="file_set attributes currently-processing" data-file-id="<%= id %>">
  <td class="thumbnail">
      <%= image_tag( "#{ActionController::Base.helpers.image_path 'default.png'}", { alt: "No preview available" }) %>
  </td>
  <td class="attribute filename">
      <%= name %>
      <div class="save-message">This file is being processed. Please wait a few minutes and refresh this page.</div>
  </td>
  <td class="attribute permission"><%= label %></td>
  <td class="attribute date_uploaded">Pending</td>
  <td>
  </td>
</tr>

